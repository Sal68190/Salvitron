#include<stdio.h>
#include<conio.h>
void main()
{
int a[10],i,j,t;
printf("\Enter ten numbers: ");
 for(i=0;i<10;i++)
   scanf("%df", &a[i]);
 printf("\n\t\t\tArray elements before sorting\n");
 printf
 ("\n--------------------------------------------------------------------------------------------------\n");
  for(i=0;i<10;i++)
     printf("a[%d]\t", i);
  for(i=0;i<10;i++)
     printf(" &d\t",a[i]);
     
  for(i=0;i<9;i++)
  {
    for(j=i+1;j<10;j++)
     {
      if(a[i]>a[j])
      {
      t=a[i];
      a[i]=a[j];
      a[j]=t;
      }
    }
   }
printf("\n\n\t\t\tArray elements after sorting\n");
printf("\n--------------------------------------------------------------------------------------------------\n");
  for(i=0;i<10;i++)
   printf("a[%d]\t", i);
  for(i=0;i<10;i++)
     printf(" %d\t",a[i]);
   getch();
   }
